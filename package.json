{
  "name": "unrolled-array",
  "version": "1.0.2",
  "description": "TypeScript implementation of an unrolled linked list for efficient memory and fast insert/delete operations.",
  "main": "dist/cjs/unrolled-linkedlist.js",
  "module": "dist/esm/unrolled-linkedlist.js",
  "types": "dist/esm/unrolled-linkedlist.d.ts",
  "exports": {
    "require": "./dist/cjs/unrolled-linkedlist.js",
    "import": "./dist/esm/unrolled-linkedlist.js"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "scripts": {
    "test": "jest unrolled-linklist.test.ts",
    "build:cjs": "tsc --project tsconfig.json",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build": "npm run build:cjs && npm run build:esm"
  },
  "files": [
    "dist/",
    "README.md"
  ],
  "keywords": [
    "typescript",
    "unrolled linked list",
    "data structure",
    "linked list",
    "efficient memory management",
    "fast operation",
    "block list",
    "array",
    "dynamic array",
    "fast insert",
    "fast deletion"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vaibhavs2/unrolled-array.git"
  },
  "author": "vaibhavs2",
  "license": "MIT"
}
